page {
  background-color: #f3f3f3;
}
.pageBox{
  overflow:hidden;
  height:100vh;

}
.swiper {
  height: 100vw;
  width: 100%;
  position: relative;
}
.swiperBox{
  height: 100%;
  position: relative
}

.swiper image {
  height: 100%;
  width: 100%;
}
.swiper .swiperNum{
  background-color: rgba(0, 0, 0, 0.5);
  font-size: 24rpx;
  color:#fff;
  text-align: center;
  height: 42rpx;
  line-height: 42rpx;
  padding: 0 20rpx;
  border-radius: 21rpx;
  position: absolute;
  bottom: 20rpx;
  right: 30rpx;
}

.buy_box {
  width: 100%;
  /* height: 110rpx; */
  position: fixed;
  bottom: 0;
  box-sizing: border-box;
  /*border-top:1rpx solid #e2e3e4; */
  background: #fff;
}

.btn_addCart {
  background: linear-gradient(90deg, #FF9C07 0%, #FF6000 100%);
  color: #fff;
  height:80rpx;
  line-height:80rpx;
  text-align:center;
  font-size:34rpx;
  font-weight: 600;
  width:390rpx;
  border-radius:50rpx;
  position:absolute;
  right:30rpx;
  top:15rpx;
}
.bgColr{
  background: linear-gradient(90deg, #FF9C07 0%, #FF6000 100%);
}
image {
  width: 100%;
}

.StoreCart {
  background: #fff;
  width: 100rpx;
  height: 110rpx;
  /* position: absolute;
  left: 0; */
  text-align: center;
  display: inline-block;
}
.CollectCart {
  background: #fff;
  width: 100rpx;
  height:110rpx;
  /* position: absolute;
  left: 100rpx; */
  text-align: center;
  display: inline-block;
}
.shoppingCart {
  background: #fff;
  width: 100rpx;
  height: 110rpx;
  /* position: absolute;
  left: 200rpx; */
  text-align: center;
  display: inline-block;
  position:relative;

}

.shoppingCart_text {
  color: #666;
  font-size: 20rpx;
  position: relative;
  top: 12rpx;
  display: block;
  line-height: 20rpx;
}

.buyNum {
  position: absolute;
  background: #E54B0C;
  color: #fff;
  font-size: 22rpx;
  border-radius: 18rpx;
  top: 8rpx;
  right: 12rpx;
  width: 30rpx;
  height: 30rpx;
  display: inline-block;
  line-height: 30rpx;
  margin-left:10rpx;
}

.buyNumbig {
  position: absolute;
  background: #E54B0C;
  color: #fff;
  font-size: 22rpx;
  border-radius: 18rpx;
  top: 8rpx;
  height: 30rpx;
  display: inline-block;
  width: 48rpx;
  line-height: 30rpx;
  right: 0rpx;
}

.proDescribe {
  padding: 38rpx 150rpx 34rpx 20rpx;
  position: relative;
  background: #fff;
  border-bottom: 1px solid #f4f4f4;
}

.proName {
  color: #333;
  font-size: 34rpx;
  line-height: 1.4;
  margin-top: 20rpx;
  font-weight:bold;
}

.proNature {
  color: #999;
  font-size: 28rpx;
  margin-top: 20rpx;
}

.present_price {
  font-size: 24rpx;
  color: #EA3323;
  margin-right: 30rpx;
  font-family: PingFangSC-Semibold;
  font-weight: bold;
  display:flex;
  align-items:flex-end;
}

.original_price {
  font-size: 24rpx;
  line-height: 30rpx;
  color: #ccc;
  text-decoration: line-through;
  margin-left: 8rpx;
  /* font-weight:400!important; */
  position: relative;
}
.original_price .spe_icon{
  background: #FFEFE5;
  border: 1rpx solid #E54B0C;
  border-radius: 4rpx;
  font-size: 20rpx;
  color: #E54B0C;
  /* line-height: 28rpx; */
  display: flex;
  align-items: center;
  justify-content: center;
  height: 28rpx;
  /* display: inline-block; */
  width: 60rpx;
  /* text-align:center; */
  margin-left:10rpx;
  position: absolute;
  right: -70rpx;
  bottom:6rpx;
}

.proPrice {
  line-height:40rpx;

}


.shareBox {
  position: absolute;
  z-index: 2;
  right:0rpx;
  top: 42rpx;
  text-align: center;
  font-size: 28rpx;
  color: #999;
  background: #F3F3F3;
  border-radius: 20px 0px 0px 20px;
  width:112rpx;
  height:40rpx;
  line-height:40rpx;

}

.shareBox button {
  width: 100%;
  height: 100%;
  position: absolute;
  opacity: 0;
  filter: alpha(opacity=0);
  z-index: 2;
}

.ico_share {
  width: 28rpx;
  height: 28rpx;
  display: inline-block;
  float:left;
  margin-right:5rpx;
  margin-top:6rpx;
  margin-left:10rpx;


}

.ico_cart {
  width: 44rpx;
  height: 44rpx;
  display: block;
  margin: 14rpx auto 0 auto;
}

.wxParse {
 padding:10px;
  background:white;
}

.attrBox {
  font-size: 30rpx;
  border-bottom: 1px solid #f4f4f4;
  background-color:white;
  overflow: hidden;
  padding:20rpx 30rpx 0rpx;

}
.attrHead{
  font-size:28rpx;
  color:#333333;
}
.attrItem{
  width: 100%;
  overflow: hidden;
  margin-top: 20rpx;
}
.attrOne {
  float: left;
  margin-right: 20rpx;
  background: #F0F0F0;
  padding: 14rpx 20rpx;
  font-size: 28rpx;
  border-radius: 4rpx;
  margin-bottom: 20rpx;
  color: #666;
}
.attrBox .active{
  background: #F39800;
  color:#fff;
}
.attrBox .noActive{
  background: #ccc;
  color:white;
}
.pro_select_box{
  font-size: 28rpx;
  color: #333333;
  height: 90rpx;
  display: flex;
  align-items: center;
  /* justify-content: flex-start; */
  background-color: #fff;
  border-bottom:2rpx solid #f0f0f0;
  font-weight: bold;
  padding-left: 20rpx;
}
.pro_select_box .b_one{
  float:left;
  width:50%;
  font-size: 31rpx;
  text-align: center;
  padding-bottom:10px;
  padding-top:10px;
}
.pro_select_box .active{
  color:#ffa52e;
  border-bottom:1px solid #ffa52e;
}
.parameters_box{
  background-color:white;
  overflow:hidden;
}
.parameters_box .b_one{
  float:left;
  padding: 0 30rpx;
  height:90rpx;
  line-height:90rpx;
  border-bottom:1rpx solid #f4f4f4;
  width:100%;
  box-sizing:border-box;
}
.parameters_box .b_one .o_left{
  float:left;
  font-size:28rpx;
  color:#999;
}
.parameters_box .b_one .o_right{
  float:left;
  text-align:left;
  font-size:28rpx;
  color:#999;
  margin-left: 20rpx;
}
.fullCutBox{
  overflow: hidden;
  margin:20rpx 0;
  position: relative;
  width: 100%;
}
.fullCutBox .b_left{
 float:left;
width:20%;
position:relative;
text-align:center;
margin-left:20rpx;

}
.fullCutBox .b_left .l_image{
 width:100%;
  height:60rpx;
}
.fullCutBox .b_left .l_span{
  position:absolute;
  font-size:28rpx;
  width:100%;
  color:white;
  top:10rpx;
}
.fullCutBox .b_right{
  float:left;
  width:70%;
}
.fullCutBox .b_right .r_span{
  font-size:30rpx;
  margin-top:10rpx;
  margin-left:20rpx;
}
.fullCutBox .b_top{
    height: 20rpx;
}
.fullCutBox .b_box{
  clear:both;
  overflow:hidden;
  height:90rpx;
  border-bottom:1rpx solid #f4f4f4;
  display: flex;
  align-items: center;
  box-sizing:border-box;
  background-color: #fff;
  /* padding: 0 20rpx; */
}
.fullCutBox .b_box .b_left_sec{
  /* float:left; */
  margin-left: 20rpx;
  margin-right:5px;
  font-weight: bold;
  /* padding:2rpx 8rpx; */
  font-size: 28rpx;
  color: #333333;
}
.fullCutBox .b_box .b_right_sec{
  /* float:left; */
  width:75%;
  font-size: 28rpx;
  color: #999;
}
.b_right_sec .b_coupon_icon{
  /* background: #FFF7DB; */
  border-radius: 2rpx;
  font-size: 24rpx;
  color: #F39800;
  display:inline-block;
  position: relative;
  padding: 0rpx 15rpx;
  line-height:40rpx;
  height:40rpx;

}
.b_right_sec .b_coupon_icon .w_dot{
 width: 16rpx;
 height: 16rpx;
 border-radius: 50%;
 background-color: #fff;
 position: absolute;
 top: 50%;
 margin-top: -8rpx;
}
.fullCutBox .b_box .b_right_icon{
  float:right;
}
.fullCutBox .b_box .bri_img{
  width:50rpx;
  height:10rpx;
  padding:20rpx 0;
}
.fullCutBox .b_box .bri_img_2{
  width:24rpx;
  height:24rpx;
  padding:34rpx 0;
}
